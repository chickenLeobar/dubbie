{
  "version": 2,
  "projects": {
    "dubbie": {
      "root": "apps/dubbie",
      "sourceRoot": "apps/dubbie",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/dubbie",
            "outputPath": "dist/apps/dubbie"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "dubbie:build",
            "dev": true,
            "port": 4201
          },
          "configurations": {
            "production": {
              "buildTarget": "dubbie:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "dubbie:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/dubbie"],
          "options": {
            "jestConfig": "apps/dubbie/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/dubbie/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "dubbie-e2e": {
      "root": "apps/dubbie-e2e",
      "sourceRoot": "apps/dubbie-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/dubbie-e2e/cypress.json",
            "tsConfig": "apps/dubbie-e2e/tsconfig.e2e.json",
            "devServerTarget": "dubbie:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "dubbie:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/dubbie-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "shared": {
      "root": "libs/shared",
      "sourceRoot": "libs/shared/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared"],
          "options": {
            "jestConfig": "libs/shared/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "sketch": {
      "root": "apps/sketch",
      "sourceRoot": "apps/sketch",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/sketch",
            "outputPath": "dist/apps/sketch"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "sketch:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "sketch:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "sketch:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/sketch"],
          "options": {
            "jestConfig": "apps/sketch/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/sketch/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "sketch-e2e": {
      "root": "apps/sketch-e2e",
      "sourceRoot": "apps/sketch-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/sketch-e2e/cypress.json",
            "tsConfig": "apps/sketch-e2e/tsconfig.e2e.json",
            "devServerTarget": "sketch:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "sketch:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/sketch-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "utils": {
      "root": "libs/utils",
      "sourceRoot": "libs/utils/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/utils/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/utils"],
          "options": {
            "jestConfig": "libs/utils/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "sketch"
}
